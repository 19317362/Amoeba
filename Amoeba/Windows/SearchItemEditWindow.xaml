                    <CheckBox Content="{Binding Path=SearchItemEditWindow_IsIgnoreCase, Source={StaticResource ResourcesInstance}}" Height="16" HorizontalAlignment="Left" Margin="138,0,0,65" Name="_nameRegexIsIgnoreCaseCheckBox" VerticalAlignment="Bottom" Width="120" />
